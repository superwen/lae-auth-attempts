{
    "name": "superwen/lae-auth-attempts",
    "description": "Add captcha and login attempts for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "auth-attempts"],
    "homepage": "https://github.com/superwen/lae-auth-attempts",
    "license": "MIT",
    "authors": [
        {
            "name": "superwen",
            "email": "superwen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6",
        "mews/captcha": "3.*",
		"aravel/ui": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Superwen\\AuthAttempts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Superwen\\AuthAttempts\\AuthAttemptsServiceProvider"
            ]

        }
    }
}
